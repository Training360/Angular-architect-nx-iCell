{
  "name": "checkout",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "checkout/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/checkout/browser",
        "index": "checkout/src/index.html",
        "main": "checkout/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "checkout/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["checkout/src/favicon.ico", "checkout/src/assets"],
        "styles": ["checkout/src/styles.scss"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "checkout/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "checkout/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4202,
        "publicHost": "http://localhost:4202"
      },
      "configurations": {
        "production": {
          "buildTarget": "checkout:build:production"
        },
        "development": {
          "buildTarget": "checkout:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "checkout:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "checkout/jest.config.ts"
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@nx/angular:webpack-server",
      "options": {
        "outputPath": "dist/checkout/server",
        "main": "checkout/server.ts",
        "tsConfig": "checkout/tsconfig.server.json",
        "inlineStyleLanguage": "scss",
        "customWebpackConfig": {
          "path": "checkout/webpack.server.config.ts"
        }
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "checkout:build:development",
          "serverTarget": "checkout:server:development"
        },
        "production": {
          "browserTarget": "checkout:build:production",
          "serverTarget": "checkout:server:production"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202
      }
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "checkout:build:development",
          "serverTarget": "checkout:server:development"
        },
        "production": {
          "browserTarget": "checkout:build:production",
          "serverTarget": "checkout:server:production"
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "checkout:build",
        "port": 4202,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "checkout:build:development"
        },
        "production": {
          "buildTarget": "checkout:build:production"
        }
      }
    },
    "static-server": {
      "dependsOn": ["build", "server"],
      "executor": "nx:run-commands",
      "options": {
        "command": "PORT=4202 node dist/checkout/server/main.js"
      }
    }
  }
}
